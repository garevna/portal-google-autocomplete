{
  "name": "portal-google-autocomplete",
  "version": "1.0.74",
  "private": false,
  "license": "MIT",
  "description": "For DGtek portal only. This package uses Google autocomplete and geolocation to check the address to be in the footprint (DGtek buildings and polygons).",
  "author": "garevna <irina.h.fylyppova@gmail.com> (https://github.com/garevna)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garevna/portal-google-autocomplete-package.git"
  },
  "scripts": {
    "dev": "webpack --mode development -w",
    "prod": "webpack --mode production",
    "package-dev": "webpack --config webpack.config.package.portal-google-autocomplete.js --mode development -w",
    "package": "webpack --config webpack.config.package.portal-google-autocomplete.js --mode production"
  },
  "files": [
    "dist/",
    "dist/css/*",
    "README.md"
  ],
  "main": "./dist/portal-google-autocomplete.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "dgtek-styles": "^0.0.8"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "node-sass": "^4.12.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/garevna/portal-google-autocomplete-package/issues"
  },
  "homepage": "https://github.com/garevna/portal-google-autocomplete-package#readme",
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "dgtek",
    "portal",
    "autocomplete",
    "google",
    "address",
    "search"
  ]
}
